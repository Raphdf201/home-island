{
    "manifest_version": 3,
    "name": "Home Island",
    "version": "1.1.0",
    "description": "A beautiful, minimalist browser extension that transforms your new tab page into a dynamic home island",
    "author": "Jerry",
    "browser_specific_settings": {
        "gecko": {
            "id": "jerryxf-home-island@github.io",
            "data_collection_permissions": {
                "required": [
                    "none"
                ]
            }
        }
    },
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "permissions": [
        "storage",
        "search"
    ],
    "options_page": "options.html",
    "icons": {
        "24": "icons/icon24.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "action": {
        "default_icon": {
            "24": "icons/icon24.png",
            "48": "icons/icon48.png",
            "128": "icons/icon128.png"
        },
        "default_title": "Home Island"
    }
}
